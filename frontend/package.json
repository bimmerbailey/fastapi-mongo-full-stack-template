{
  "name": "frontend",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "pretty": "prettier --config .prettierrc.json -l 'src/**/*.{js,ts,css,scss,md,vue}'",
    "lint:all": "yarn lint && yarn pretty",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "pretty:fix": "prettier --config .prettierrc.json -w 'src/**/*.{js,ts,css,scss,md,vue}'",
    "format": "yarn lint:fix && yarn pretty:fix"
  },
  "dependencies": {
    "@mdi/js": "^7.2.96",
    "@popperjs/core": "^2.11.7",
    "apexcharts": "^3.40.0",
    "axios": "^1.4.0",
    "chart.js": "^4.3.0",
    "moment": "^2.29.4",
    "numeral": "^2.0.6",
    "pinia": "^2.1.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.14",
    "@types/numeral": "^2.0.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "eslint": "8.41.0",
    "eslint-plugin-vue": "^9.13.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.0.4",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.3.8",
    "vue-tsc": "^1.7.6"
  }
}
